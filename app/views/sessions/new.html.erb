<div class="loginPage">
  <div class="loginBox">
    <h1>Login</h1>
    <form  action="<%= session_url %>" method="post">
      <%= auth_token_input %>

      <label>Email
        <input type="text" name="user[email]" value="">
      </label>
      <br/>
      <label>
        Password
        <input type="password" name="user[password]" value="">
      </label>
      <br/>

      <input type="submit"  value="Login">
    </form>
  </div>
</div>
