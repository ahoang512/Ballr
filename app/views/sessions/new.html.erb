<div class="loginPage">
  <div class="loginBox">
    <h1>Login</h1>
    <form  action="<%= session_url %>" method="post">
      <%= auth_token_input %>

      <label class="email">Email
      </label>
        <input type="text" name="user[email]" value="">
      <br/>
      <label>
        Password
        <input type="password" name="user[password]" value="">
      </label>
      <br/>

      <input type="submit"  id="loginButton" value="Login">
    </form>
  </div>
</div>
